<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1;net6.0;net7.0;net8.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<SignAssembly>True</SignAssembly>
		<AssemblyOriginatorKeyFile>..\DispatchProxyAdvanced.snk</AssemblyOriginatorKeyFile>
		<AssemblyVersion>2.0.6</AssemblyVersion>
		<FileVersion>2.0.6</FileVersion>
		<Version>2.0.6</Version>
		<Company></Company>
		<Authors>Leonid Salavatov</Authors>
		<Copyright>Leonid Salavatov 2024</Copyright>
		<PackageId>DispatchProxyAdvanced</PackageId>
		<Product>DispatchProxyAdvanced</Product>
		<Title>DispatchProxyAdvanced</Title>
		<Description>Extended version of DispatchProxy with Class proxying and custom states</Description>
		<PackageTags>dispatchproxy dispatch-proxy proxy-factory proxy-object proxy-class proxy-interface proxy-handler proxy-state custom-state</PackageTags>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageProjectUrl>https://github.com/mustaddon/DispatchProxyAdvanced</PackageProjectUrl>
		<RepositoryUrl>https://github.com/mustaddon/DispatchProxyAdvanced</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<NeutralLanguage />
		<PackageReleaseNotes></PackageReleaseNotes>
		<AllowUnsafeBlocks>False</AllowUnsafeBlocks>
	</PropertyGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
		<PackageReference Include="System.Reflection.DispatchProxy" Version="4.7.1" />
		<PackageReference Include="System.Reflection.Emit" Version="4.7.0" />
	</ItemGroup>

</Project>
